{
  "name": "react-causality-redux-vscode-template",
  "version": "1.0.0",
  "description": "A Webpack tempplate with hot reloading React components with causality-redux for react, error handling on module and component level and Visual Studio Code integration",
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production webpack --colors --config devtools/webpack.config.prod.js",
    "builddll:dev": "webpack --colors --config devtools/webpack.config.dlllib.dev.js",
    "builddll:prod": "cross-env NODE_ENV=production webpack --colors --config devtools/webpack.config.dlllib.prod.js",
    "build": "npm run build:prod",
    "builddll": "npm run builddll:dev && npm run builddll:prod",
    "build:all": "npm run builddll:dev && npm run builddll:prod && npm run build",
    "start": "cross-env NODE_ENV=development node devtools/devServer.js",
    "startprod": "cross-env NODE_ENV=production node devtools/prodServer.js",
    "lint": "eslint src/**",
    "test": "mocha --require ignore-styles --require test/setup.js test/**/*-test.js",
    "compile:del": "rimraf temp",
    "compile:copy": "copyfiles src/**/*.* temp",
    "compile:src": "babel ./test/**/*.js ./src/**/*.js ./src/**/*.jsx --out-dir temp --source-maps --watch",
    "buildmochadebug": "npm run compile:del && npm run compile:copy && npm run compile:src",
    "mochadebug": "npm run compile:del && npm run compile:copy && npm run compile:testjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrewBanks10/react-causality-redux-vscode-template"
  },
  "keywords": [
    "causality-redux",
    "react-causality-redux",
    "react",
    "reactjs",
    "tempplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "enzyme",
    "mocha"
  ],
  "author": "Andrew Banks",
  "license": "MIT",
  "homepage": "https://github.com/AndrewBanks10/react-causality-redux-vscode-template",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-lodash": "^2.4.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "ghooks": "^2.0.0",
    "html-webpack-plugin": "^2.29.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.0.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "~3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "causality-redux": "^2.1.6",
    "react": "^15.6.1",
    "react-causality-redux": "^2.0.8",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "redux": "^3.7.1"
  },
  "config": {
    "host": "localhost",
    "port": 3000,
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  }
}
